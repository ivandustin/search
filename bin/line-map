#!/usr/bin/env python3
from sys import argv, stdin


def main():
    filepath = argv[1]
    index = []
    with open(filepath, "r", encoding="utf-8") as file:
        for line in file:
            index.append(line.strip())
    for line in stdin:
        print(" ".join([index[int(i) - 1] for i in line.strip().split(" ")]))


if __name__ == "__main__":
    main()
